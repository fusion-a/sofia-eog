#This file defines a filter for the sofia-eog library
#See the help on defining filters at FIXME

#Static
Exec=/sync/sofia-eog/bin/EOGfilter
Arguments=-i InputSampleRate -o OutputSampleRate -c 50 -f FlushFrequency
Identifier=sum_filter
InputIdentifier=raw_input
OutputSampleRate=500
SampleSize=4

#Dynamic (can be changed by the program)
RecordsToFile=1
DisplaySampleRate=100
FlushFrequency=500
IsEnabled=1
